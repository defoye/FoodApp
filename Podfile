  pod 'GoogleSignIn'
  pod 'Firebase/Analytics'
  pod 'Firebase/Core'
  pod 'FirebaseUI/Google'
  pod 'QuiteAdaptableKit', '~> 0.0.2'

  target 'FoodAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

